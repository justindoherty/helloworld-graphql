{
  "name": "helloworld-graphql",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "graphql:download-schema": "apollo-codegen download-schema http://localhost:4000/graphql --output schema.json",
    "graphql:generate-types": "apollo-codegen generate src/app/**/*.graphql --schema schema.json --target typescript --output src/app/schema.ts",
    "graphql": "npm-run-all -c graphql:download-schema graphql:generate-types"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.0",
    "@angular/compiler": "^2.4.0",
    "@angular/core": "^2.4.0",
    "@angular/forms": "^2.4.0",
    "@angular/http": "^2.4.0",
    "@angular/platform-browser": "^2.4.0",
    "@angular/platform-browser-dynamic": "^2.4.0",
    "@angular/router": "^3.4.0",
    "apollo-angular": "^0.11.0",
    "apollo-client": "^0.9.0",
    "core-js": "^2.4.1",
    "graphql-tag": "^1.2.4",
    "rxjs": "^5.0.1",
    "subscriptions-transport-ws": "^0.5.3",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-beta.31",
    "@angular/compiler-cli": "^2.4.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "apollo-codegen": "^0.10.8",
    "codelyzer": "~2.0.0-beta.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "^1.0.2",
    "npm-run-all": "^4.0.2",
    "protractor": "~5.1.0",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.2.1"
  }
}
